{
	"$schema": "https://developer.microsoft.com/json-schemas/tsdoc/v0/tsdoc.schema.json",
	"tagDefinitions": [
		{
			"tagName": "@info",
			"syntaxKind": "block"
		},
		{
			"tagName": "@warning",
			"syntaxKind": "block"
		},
		{
			"tagName": "@error",
			"syntaxKind": "block"
		},
		{
			"tagName": "@since",
			"syntaxKind": "block",
			"allowMultiple": false
		},
		{
			"tagName": "@author",
			"syntaxKind": "block"
		},
		{
			"tagName": "@component",
			"syntaxKind": "modifier",
			"allowMultiple": false
		},
		{
			"tagName": "@props",
			"syntaxKind": "modifier",
			"allowMultiple": false
		},
		{
			"tagName": "@hook",
			"syntaxKind": "modifier",
			"allowMultiple": false
		},
		{
			"tagName": "@betaDocumentation",
			"syntaxKind": "modifier"
		},
		{
			"tagName": "@internalRemarks",
			"syntaxKind": "block"
		},
		{
			"tagName": "@preapproved",
			"syntaxKind": "modifier"
		}
	],
	"supportForTags": {
		"@info": true,
		"@warning": true,
		"@error": true,
		"@since": true,
		"@component": true,
		"@props": true,
		"@hook": true,

		"@alpha": true,
		"@beta": true,
		"@defaultValue": true,
		"@decorator": true,
		"@deprecated": true,
		"@eventProperty": true,
		"@example": true,
		"@experimental": true,
		"@inheritDoc": true,
		"@internal": true,
		"@label": true,
		"@link": true,
		"@override": true,
		"@packageDocumentation": true,
		"@param": true,
		"@privateRemarks": true,
		"@public": true,
		"@readonly": true,
		"@remarks": true,
		"@returns": true,
		"@sealed": true,
		"@see": true,
		"@throws": true,
		"@typeParam": true,
		"@virtual": true,

		"@betaDocumentation": true,
		"@internalRemarks": true,
		"@preapproved": true
	}
}
